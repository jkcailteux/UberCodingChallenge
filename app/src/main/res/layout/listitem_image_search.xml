<?xml version="1.0" encoding="utf-8"?>

<com.jeffcailteux.ubercodingchallenge.views.listitem.ImageSearchListItem xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginTop="10dp"
    android:orientation="horizontal"
    android:background="#fff"
    android:weightSum="3">

    <com.android.volley.toolbox.NetworkImageView
        android:id="@+id/listitem_image_search_iv1"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="#000"
        android:layout_weight="1"
        />
    <com.android.volley.toolbox.NetworkImageView
        android:id="@+id/listitem_image_search_iv2"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="#000"
        android:layout_marginLeft="10dp"
        android:layout_weight="1" />
    <com.android.volley.toolbox.NetworkImageView
        android:id="@+id/listitem_image_search_iv3"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="#000"
        android:layout_marginLeft="10dp"
        android:layout_weight="1" />

</com.jeffcailteux.ubercodingchallenge.views.listitem.ImageSearchListItem >